{
  "name": "@morsedigital/collapsible_content_vanilla",
  "version": "1.0.1",
  "description": "Vanilla JS collapsible content",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js' --source-maps  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "test": "npm run lint && ./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "test:mac": "karma start --single-run --browsers Chrome,Firefox,Safari",
    "test:watch": "karma start --browsers PhantomJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morsedigital/collapsible_content_vanilla.git"
  },
  "keywords": [
    "collapsible content"
  ],
  "author": "Ian Hatton",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morsedigital/collapsible_content_vanilla/issues"
  },
  "homepage": "https://github.com/morsedigital/collapsible_content_vanilla#readme",
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-browserify": "^5.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-story-reporter": "^0.3.1",
    "lodash": "^4.6.0",
    "phantomjs-prebuilt": "^2.1.4",
    "rewireify": "^0.2.5",
    "viewport-detection-es6": "^2.0.3",
    "watchify": "^3.7.0"
  },
  "peerDependencies": {
    "lodash": "^4.6.0",
    "viewport-detection-es6": "^2.0.3"
  }
}
